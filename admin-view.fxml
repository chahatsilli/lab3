<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="559.0" prefWidth="772.0" spacing="20.0" xmlns="http://javafx.com/javafx/23" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.hrmanagement.adminController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <AnchorPane prefHeight="200.0" prefWidth="200.0">
        <children>
            <Label fx:id="password" />
         <TableView fx:id="tableview" layoutX="270.0" layoutY="1.0" prefHeight="200.0" prefWidth="376.0">
           <columns>
             <TableColumn prefWidth="75.0" fx:id="admin_id" text="admin_id" />
             <TableColumn prefWidth="75.0" fx:id="name" text="name" />
               <TableColumn prefWidth="75.0" fx:id="email" text="email" />
               <TableColumn prefWidth="75.0" fx:id="pasword" text="password"/>
           </columns>
         </TableView>
         <Button onAction="#deletedata" layoutX="14.0" layoutY="100.0" mnemonicParsing="false" text="delete" />
         <Button onAction="#viewdata" layoutX="17.0" layoutY="150.0" mnemonicParsing="false" text="view" />
         <Button onAction="#updatedata" layoutX="13.0" layoutY="58.0" mnemonicParsing="false" text="update" />
         <Button onAction="#insertdata" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" text="insert" />
         <Button onAction="#backdata" layoutX="25.0"  layoutY="25.0" mnemonicParsing="false" text="back"/>
         <TextField fx:id="admin" layoutX="102.0" layoutY="14.0" prefHeight="25.0" prefWidth="63.0" />
         <TextField fx:id="nam" layoutX="94.0" layoutY="58.0" prefHeight="25.0" prefWidth="101.0" />
         <TextField fx:id="emai" layoutX="85.0" layoutY="100.0" />
         <TextField fx:id="passwor" layoutX="85.0" layoutY="150.0" />
        </children>
    </AnchorPane>
</VBox>
